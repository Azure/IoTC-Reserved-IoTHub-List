  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.2",
    "prettyjson": "^1.2.1",
    "request": "^2.88.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^12.0.11",
    "@types/prettyjson": "0.0.29",
    "@types/yargs": "^13.0.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.2"
  }
}
